                                <a href="/dashboard/categories/add">Ajouter</a>
                            </li>
                        </ul>
                    </li>
                    <li class="<%= menu == 'location' ? 'active' : '' %>">
                        <a href="/dashboard/locations/pays"  class="menu-toggle">
                            <i class="menu-icon ti-location-pin"></i>
                            <span>Pays, regions et quartiers</span>
                        </a>
                       
                    </li>
                   
                    <li class="<%= menu == 'product' ? 'active' : '' %>">
                        <a href="/dashboard/products" class="menu-toggle">
